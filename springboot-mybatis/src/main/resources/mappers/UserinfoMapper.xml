<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springbootmybatis.dao.UserinfoMapper">

    <select id="getUserinfoByName" resultType="com.springbootmybatis.entity.SyUserinfo">
        select * from sy_userinfo where username=#{name}
    </select>

</mapper>